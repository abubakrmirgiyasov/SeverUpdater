<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--theme colors-->
    <Color x:Key="LightColor">#CBE4DE</Color>
    <Color x:Key="DarkColor">#2C3333</Color>
    <Color x:Key="MutedDarkColor">#2E4F4F</Color>
    <Color x:Key="MutedLightColor">#0E8388</Color>

    <!--solid brushes-->
    <SolidColorBrush x:Key="LightColorBrush" Color="#CBE4DE" />
    <SolidColorBrush x:Key="DarkColorBrush" Color="#2C3333" />
    <SolidColorBrush x:Key="MutedDarkBrush" Color="#2E4F4F" />
    <SolidColorBrush x:Key="MutedLightBrush" Color="#0E8388" />

    <!--font sizes-->
    <Style x:Key="Fs-12" TargetType="{x:Type Control}">
        <Setter Property="FontSize" Value="12"/>
    </Style>
    <Style x:Key="Fs-14" TargetType="{x:Type Control}">
        <Setter Property="FontSize" Value="14"/>
    </Style>
    <Style x:Key="Fs-16" TargetType="{x:Type Control}">
        <Setter Property="FontSize" Value="16"/>
    </Style>
    <Style x:Key="Fs-18" TargetType="{x:Type Control}">
        <Setter Property="FontSize" Value="16"/>
    </Style>
    <Style x:Key="Fs-20" TargetType="{x:Type Control}">
        <Setter Property="FontSize" Value="20"/>
    </Style>
</ResourceDictionary>