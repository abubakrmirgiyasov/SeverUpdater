<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--window style-->
    <Style TargetType="Window" x:Key="Window">
        <Setter Property="Background" Value="#CBE4DE" />
        <Setter Property="Foreground" Value="#2C3333" />
    </Style>

    <!--header-->
    <Style TargetType="Border" x:Key="Header">
        <Setter Property="Background" Value="{DynamicResource DarkColorBrush}" />
    </Style>
</ResourceDictionary>